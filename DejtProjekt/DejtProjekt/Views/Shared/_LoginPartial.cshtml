@using Microsoft.AspNet.Identity
@if (Request.IsAuthenticated)
{
    using (Html.BeginForm("LogOut", "LoggIn", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
    {
    @Html.AntiForgeryToken()

    <ul class="nav navbar-nav navbar-right">
        <li>
            @Html.ActionLink("Hello " + User.Identity.GetUserName() + "!", "Details" + "/" + Session["UserID"].ToString(), "UserModels", routeValues: null, htmlAttributes: new { title = "Manage" })
        </li>
        <li><a href="@Url.Action("Logout", "LoggIn")">Logout</a></li>
    </ul>
    }
}
else
{
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink("Registrera", "Register", "LoggIn", routeValues: null, htmlAttributes: new { id = "registerLink" })</li>
        <li>@Html.ActionLink("Logga in", "Login", "LoggIn", routeValues: null, htmlAttributes: new { id = "loginLink" })</li>
    </ul>
}
